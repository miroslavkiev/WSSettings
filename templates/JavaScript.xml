      <option name="JAVA_SCRIPT" value="true" />
      <option name="JSON" value="true" />
    </context>
  </template>
  <template name="ifel" value="if($STATEMENT$){&#10;  $IFTRUE$&#10;} else {&#10;  $IFFALSE$&#10;}" description="inserts if .. else statement" toReformat="false" toShortenFQNames="true">
