    <variable name="IFFALSE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JS_CLASS" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="func" value="function $FUNCTION_NAME$($FUNCTION_PARAMS$) {&#10;  $END$&#10;};" description="function expression" toReformat="false" toShortenFQNames="true">
